{
  "name": "kanji-typesetting",
  "type": "module",
  "version": "1.0.0",
  "description": "A JavaScript library for Japanese/Chinese typography and typesetting",
  "author": "syhily",
  "license": "MIT",
  "homepage": "https://github.com/syhily/kanji-typesetting#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syhily/kanji-typesetting.git"
  },
  "bugs": {
    "url": "https://github.com/syhily/kanji-typesetting/issues"
  },
  "keywords": [
    "typography",
    "typesetting",
    "japanese",
    "chinese",
    "kanji"
  ],
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    },
    "./style.css": "./dist/main.css"
  },
  "main": "./dist/main.cjs",
  "types": "./dist/main.d.ts",
  "files": [
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint",
    "test": "vitest run"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.0",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.0.2",
    "jiti": "^2.6.1",
    "jsdom": "^27.1.0",
    "sass-embedded": "^1.93.3",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.2",
    "vitest": "^4.0.8"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.2"
  }
}
